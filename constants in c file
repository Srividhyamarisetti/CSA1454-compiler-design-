digit [0-9]
%{
int cons=0;
%}pq;a
%%
{digit}+ {cons++; printf("%s is a constant\n", yytext);  }
.|\n 

%%
int yywrap(void) {
return 1; }
int main(void)
{
yyin = fopen("factoril.txt","r");
yylex();
printf("Number of Constants : %d\n", cons);
fclose(yyin);  
}

